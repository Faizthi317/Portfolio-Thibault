{
  "ci": {
    "collect": {
      "staticDistDir": ".",
      "url": [
        "http://localhost/index.html",
        "http://localhost/src/parcours/parcours.html",
        "http://localhost/src/contact/contact.html"
      ],
      "numberOfRuns": 1
    },
    "assert": {
      "assertions": {
        "categories:performance": [
          "error",
          {
            "minScore": 0.8
          }
        ],
        "categories:accessibility": [
          "error",
          {
            "minScore": 0.8
          }
        ],
        "categories:best-practices": [
          "error",
          {
            "minScore": 0.8
          }
        ],
        "categories:seo": [
          "error",
          {
            "minScore": 0.8
          }
        ]
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}
